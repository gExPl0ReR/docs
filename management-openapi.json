  /kvm/v2/set/reinstall:
    post:
      tags:
        - Installation
      summary: reinstall
      requestBody:
        content:
          application/x-www-form-urlencoded:
            schema:
              properties:
                signature:
                  type: string
                  description: get data from "get session" call
                  example: '{{signature}}'
                kvm_id:
                  type: string
                  description: a kvm id
                  example: kvm_xxxxx_yyyyy
                valid_until:
                  type: string
                  description: get data from "get session" call
                  example: '{{valid_until}}'
                image:
                  type: string
                  description: see "get image" call
                  example: debian-buster
                password:
                  type: string
                  description: password for root login
                  example: new root password
                pubkey:
                  type: string
                  description: (deprecated) optional root key
                  example: ssh-rsa ...
                postscript:
                  type: string
                  description: >-
                    (deprecated) script is going to be executed after
                    installation
                  example: '#!/bin/sh\n....'
                cloudinit_user_data:
                  type: string
                  description: (optional) for use with cloudinit_* images
                  example: |
                    #cloud-config
                    set_hostname: YOUR_HOSTNAME
                    user: root
                    password: YOUR_ROOT_PASSWORD
                    disable_root: false
                    disable_root_opts: ''
                    ssh_pwauth: True
                    manage_etc_hosts: true
                    expire: False
                    runcmd:
                      - chage -d 19000 root
                      - [ /usr/bin/sed, -i, -E, 's/disable_root:.*true/disable_root: false/i', /etc/cloud/cloud.cfg ]
                      - [ /usr/bin/sed, -i, -E, 's/#PermitRootLogin.*/PermitRootLogin yes/i', /etc/ssh/sshd_config ]
                    users:
                    - default
                    power_state:
                     delay: now
                     mode: reboot
                     message: Bye Bye
                     timeout: 30
                     condition: True
                    package_upgrade: true
      responses:
        '200':
          description: OK
          content:
            text/plain:
              schema:
                type: string
              example: '{"status":"success"}'

  /kvm/v2/add/order:
    post:
      tags:
        - default
      summary: add order
      requestBody:
        content:
          application/x-www-form-urlencoded:
            schema:
              properties:
                email:
                  type: string
                  description: your whmcs credentials
                  example: 'your email '
                pw:
                  type: string
                  description: your whmcs credentials
                  example: password
                order:
                  type: string
                  description: json encoded
                  example: >-
                    {"billingcycle":"monthly","paymentmethod":"paypal","item":[{"pid":1814,"os":92,"hostname":"kvmtest","additional_ram":467,"additional_vcpu":467,"additional_ip":467,"additional_diskboost":615},{"pid":1814,"os":88,"hostname":"kvmtest2","additional_ram":"","additional_vcpu":"","additional_ip":"","additional_driveboost":""}]}
      security:
        - noauthAuth: []
      responses:
        '200':
          description: OK
          content:
            text/plain:
              schema:
                type: string
              example: >-
                {"orderid":"2....8","serviceids":"2...0,....","invoiceid":"1...0","status":"success"}
  /kvm/v2/cancel/product:
    post:
      tags:
        - default
      summary: cancel product
      requestBody:
        content:
          application/x-www-form-urlencoded:
            schema:
              properties:
                email:
                  type: string
                  description: your whmcs credentials
                  example: 'your email '
                pw:
                  type: string
                  description: your whmcs credentials
                  example: password
                kvmid:
                  type: string
                  description: kvmid to cancel if serviceid unknown
                  example: kvm_xxxxx_yyyyy
                serviceid:
                  type: integer
                  description: serviceid to cancel if kvmid unknown
                  example: '1234'
                type:
                  type: string
                  description: >-
                    The type of cancellation. â€˜Immediateâ€™ or â€˜End of Billing
                    Periodâ€™
                  example: End of Billing Period
      security:
        - noauthAuth: []
      responses:
        '200':
          description: OK
          content:
            text/plain:
              schema:
                type: string
              example: >-
                {"orderid":"2....8","serviceids":"2...0,....","invoiceid":"1...0","status":"success"}
